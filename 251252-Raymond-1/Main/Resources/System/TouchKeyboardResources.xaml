xmlns:system="clr-namespace:System;assembly=mscorlib">

	<local:TouchKeyboardLayout x:Key="TouchKeyboardLayout">

		<local:TouchKeyboardLayout.DeadKeys>
			<system:String>´</system:String>
			<system:String>`</system:String>
			<system:String>^</system:String>
		</local:TouchKeyboardLayout.DeadKeys>

		<local:TouchKeyboardLayout.TouchKeys>
            <!--Zahlenreihe -->
			<local:TouchKey	Name="TKM_Button01" Regular="^" Shift="°" TextBottomLeft="^" TextTopLeft="°"/>
			<local:TouchKey	Name="TKM_Key02" Regular="1" Shift="!" TextBottomLeft="1" TextTopLeft="!"/>
			<local:TouchKey	Name="TKM_Key03" Regular="2" Shift="&quot;" AltGr="²" TextBottomLeft="2" TextTopLeft="&quot;" TextBottomRight="²"/>
			<local:TouchKey	Name="TKM_Key04" Regular="3" Shift="§" AltGr="³" TextBottomLeft="3" TextTopLeft="§" TextBottomRight="³"/>
			<local:TouchKey	Name="TKM_Key05" Regular="4" Shift="$" TextBottomLeft="4" TextTopLeft="$"/>
			<local:TouchKey	Name="TKM_Key06" Regular="5" Shift="%" TextBottomLeft="5" TextTopLeft="%"/>
			<local:TouchKey	Name="TKM_Key07" Regular="6" Shift="&amp;" TextBottomLeft="6" TextTopLeft="&amp;"/>
			<local:TouchKey	Name="TKM_Key08" Regular="7" Shift="/" AltGr="{}{" TextBottomLeft="7" TextTopLeft="/" TextBottomRight="{}{" />
			<local:TouchKey	Name="TKM_Key09" Regular="8" Shift="(" AltGr="[" TextBottomLeft="8" TextTopLeft="(" TextBottomRight="["/>
			<local:TouchKey	Name="TKM_Key10" Regular="9" Shift=")" AltGr="]" TextBottomLeft="9" TextTopLeft=")" TextBottomRight="]"/>
			<local:TouchKey	Name="TKM_Key11" Regular="0" Shift="=" AltGr="}" TextBottomLeft="0" TextTopLeft="=" TextBottomRight="}"/>
			<local:TouchKey	Name="TKM_Key12" Regular="ß" Shift="?" AltGr="\" TextTopLeft="?" TextBottomLeft="ß" TextBottomRight="\" />
			<local:TouchKey	Name="TKM_Key13" Regular="'" Shift="'" TextBottomLeft="'" TextTopLeft="'"/>
			<local:TouchKey	Name="TKM_Back" TextCenter="←"/>

            <!-- Q bis + -->
			<local:TouchKey	Name="TKM_Tab" TextTopLeft="←" TextBottomLeft="→"/>
			<local:TouchKey	Name="TKM_Key14" Regular="q" Shift="Q" TextCenter="Q"/>
			<local:TouchKey	Name="TKM_Key15" Regular="w" Shift="W" TextCenter="W"/>
			<local:TouchKey	Name="TKM_Key16" Regular="e" Shift="E" TextCenter="E"/>
			<local:TouchKey	Name="TKM_Key17" Regular="r" Shift="R" TextCenter="R"/>
			<local:TouchKey	Name="TKM_Key18" Regular="t" Shift="T" TextCenter="T"/>
			<local:TouchKey	Name="TKM_Key19" Regular="z" Shift="Z" TextCenter="Z"/>
			<local:TouchKey	Name="TKM_Key20" Regular="u" Shift="U" TextCenter="U"/>
			<local:TouchKey	Name="TKM_Key21" Regular="i" Shift="I" TextCenter="I"/>
			<local:TouchKey	Name="TKM_Key22" Regular="o" Shift="O" TextCenter="O"/>
			<local:TouchKey	Name="TKM_Key23" Regular="p" Shift="P" TextCenter="P"/>
			<local:TouchKey	Name="TKM_Key24" Regular="ü" Shift="Ü" TextCenter="Ü"/>
			<local:TouchKey	Name="TKM_Key25" Regular="+" Shift="*" AltGr="~" TextTopLeft="*" TextBottomLeft="+" TextBottomRight="~" />
			<local:TouchKey	Name="TKM_Return" TextCenter="Enter"/>

            <!-- A bis # -->
			<local:TouchKey	Name="TKM_Capital" TextCenter="↓" Regular="↓" Shift="↑" />
			<local:TouchKey	Name="TKM_Key26" Regular="a" Shift="A" TextCenter="A"/>
			<local:TouchKey	Name="TKM_Key27" Regular="s" Shift="S" TextCenter="S"/>
			<local:TouchKey	Name="TKM_Key28" Regular="d" Shift="D" TextCenter="D"/>
			<local:TouchKey	Name="TKM_Key29" Regular="f" Shift="F" TextCenter="F"/>
			<local:TouchKey	Name="TKM_Key30" Regular="g" Shift="G" TextCenter="G"/>
			<local:TouchKey	Name="TKM_Key31" Regular="h" Shift="H" TextCenter="H"/>
			<local:TouchKey	Name="TKM_Key32" Regular="j" Shift="J" TextCenter="J"/>
			<local:TouchKey	Name="TKM_Key33" Regular="k" Shift="K" TextCenter="K"/>
			<local:TouchKey	Name="TKM_Key34" Regular="l" Shift="L" TextCenter="L"/>
			<local:TouchKey	Name="TKM_Key35" Regular="ö" Shift="Ö" TextCenter="Ö"/>
			<local:TouchKey	Name="TKM_Key36" Regular="ä" Shift="Ä" TextCenter="Ä"/>
			<local:TouchKey	Name="TKM_Key37" Regular="#" Shift="'" TextBottomLeft="#" TextTopLeft="'"/>

            <!-- < bis _ -->
			<local:TouchKey	Name="TKM_LeftShift" Regular="↑" TextCenter="↑"/>
			<local:TouchKey	Name="TKM_Key38" Regular="&lt;" Shift="&gt;" AltGr="|" TextTopLeft="&gt;" TextBottomLeft="&lt;" TextBottomRight="|"/>
			<local:TouchKey	Name="TKM_Key39" Regular="y" Shift="Y" TextCenter="Y"/>
			<local:TouchKey	Name="TKM_Key40" Regular="x" Shift="X" TextCenter="X"/>
			<local:TouchKey	Name="TKM_Key41" Regular="c" Shift="C" TextCenter="C"/>
			<local:TouchKey	Name="TKM_Key42" Regular="v" Shift="V" TextCenter="V"/>
			<local:TouchKey	Name="TKM_Key43" Regular="b" Shift="B" TextCenter="B"/>
			<local:TouchKey	Name="TKM_Key44" Regular="m" Shift="M" AltGr="µ" TextCenter="M" TextBottomRight="µ"/>
			<local:TouchKey	Name="TKM_Key45" Regular="n" Shift="N" TextCenter="N"/>
			<local:TouchKey	Name="TKM_Key46" Regular="," Shift=";" TextBottomLeft="," TextTopLeft=";"/>
			<local:TouchKey	Name="TKM_Key47" Regular="." Shift=":" TextBottomLeft="." TextTopLeft=":"/>
			<local:TouchKey	Name="TKM_Key48" Regular="-" Shift="_" TextBottomLeft="-" TextTopLeft="_"/>
			<local:TouchKey	Name="TKM_RightShift" Regular="↑" TextCenter="↑"/>

            <!-- SpaceReihe -->
			<local:TouchKey	Name="TKM_LeftCtrl" Regular="Strg" Shift="Strg" TextCenter="Strg"/>
			<local:TouchKey	Name="TKM_LeftAlt" Regular="Alt" Shift="Alt" TextCenter="Alt"/>
			<local:TouchKey	Name="TKM_Space"/>
			<local:TouchKey	Name="TKM_RightAlt" Regular="AltGr" Shift="AltGr" TextCenter="AltGr"/>
			<local:TouchKey	Name="TKM_RightCtrl" Regular="Strg" Shift="Strg" TextCenter="Strg"/>

			<local:TouchKey	Name="TKM_Oem3" Regular="'" Shift="~" TextBottomLeft="'" TextTopLeft="'"/>
			<local:TouchKey	Name="TKM_OemQuotes" Regular="," Shift="&quot;" TextBottomLeft="," TextTopLeft="&quot;"/>

			<local:TouchKey	Name="TKM_Clear"/>
			<local:TouchKey	Name="TKM_Delete" TextCenter="Del"/>

			<local:TouchKey	Name="TKM_NumPad0" Regular="0"/>
			<local:TouchKey	Name="TKM_NumPad1" Regular="1"/>
			<local:TouchKey	Name="TKM_NumPad2" Regular="2"/>
			<local:TouchKey	Name="TKM_NumPad3" Regular="3"/>
			<local:TouchKey	Name="TKM_NumPad4" Regular="4"/>
			<local:TouchKey	Name="TKM_NumPad5" Regular="5"/>
			<local:TouchKey	Name="TKM_NumPad6" Regular="6"/>
			<local:TouchKey	Name="TKM_NumPad7" Regular="7"/>
			<local:TouchKey	Name="TKM_NumPad8" Regular="8"/>
			<local:TouchKey	Name="TKM_NumPad9" Regular="9"/>

			<local:TouchKey	Name="TKM_Cancel"/>
			<local:TouchKey	Name="TKM_Enter"/>
			<local:TouchKey	Name="TKM_Escape"/>
			<local:TouchKey	Name="TKM_CapsLock" TextCenter="↑"/>

            <local:TouchKey	Name="TKM_Nums" TextCenter="123"/>
            <local:TouchKey	Name="TKM_Marks" TextCenter="#+="/>
			
			<local:TouchKey	Name="TKM_Up" TextCenter="↑"/>
			<local:TouchKey	Name="TKM_Right" TextCenter="→"/>
			<local:TouchKey	Name="TKM_Down" TextCenter="↓"/>
			<local:TouchKey	Name="TKM_Left" TextCenter="←"/>

			<local:TouchKey	Name="TKM_F1"/>
			<local:TouchKey	Name="TKM_F2"/>
			<local:TouchKey	Name="TKM_F3"/>
			<local:TouchKey	Name="TKM_F4"/>
			<local:TouchKey	Name="TKM_F5"/>
			<local:TouchKey	Name="TKM_F6"/>
			<local:TouchKey	Name="TKM_F7"/>
			<local:TouchKey	Name="TKM_F8"/>
			<local:TouchKey	Name="TKM_F9"/>
			<local:TouchKey	Name="TKM_F10"/>
			<local:TouchKey	Name="TKM_F11"/>
			<local:TouchKey	Name="TKM_F12"/>
			<local:TouchKey	Name="TKM_F13"/>
			<local:TouchKey	Name="TKM_F14"/>
			<local:TouchKey	Name="TKM_F15"/>
			<local:TouchKey	Name="TKM_F16"/>
			<local:TouchKey	Name="TKM_F17"/>
			<local:TouchKey	Name="TKM_F18"/>
			<local:TouchKey	Name="TKM_F19"/>
			<local:TouchKey	Name="TKM_F20"/>
			<local:TouchKey	Name="TKM_F21"/>
			<local:TouchKey	Name="TKM_F22"/>
			<local:TouchKey	Name="TKM_F23"/>
			<local:TouchKey	Name="TKM_F24"/>

		</local:TouchKeyboardLayout.TouchKeys>
	</local:TouchKeyboardLayout>

