                        <vw:NumericVarIn x:Name="vh" Style="{DynamicResource NVI_Main}" FontWeight="Bold" Height="45" Margin="0,5,0,0"  LocalizableUnitText="@Units.h" RawLimitMax="10" RawLimitMin="1" FormatString="{}0" VariableName="CPU1.PLC.Blocks.03 Basket coating.02 DT.00 Main.DB DT PD.Station.DT.Viscosity.Time.H" AuthorizationRight="RecipePP" />
