                    </StackPanel>
                    <vw:NumericVarOut Style="{DynamicResource NVO_Main}" Width="200" FontWeight="Bold" Height="145" FontSize="25" LocalizableUnitText="@Units.s" LocalizableLabelText="@MachineOverview.Text28" RawLimitMax="59" RawLimitMin="0" FormatString="{}0" VariableName="CPU3.PLC.Blocks.03 Basket coating.01 CD.00 Main.Program.DB Program control.Actual.Spinning.2.Time"/>
                </StackPanel>
            </vw:RadioButton>
            <vw:RadioButton x:Name="s3" Style="{DynamicResource RB_Group}" IsEnabled="False"  Margin="-2" LocalizableLabelText="@RecipeSystem.Text54" >
