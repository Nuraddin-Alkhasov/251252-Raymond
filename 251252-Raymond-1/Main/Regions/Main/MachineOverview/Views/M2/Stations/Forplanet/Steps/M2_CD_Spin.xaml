                    <vw:NumericVarOut Style="{DynamicResource NVO_Main}" Width="200" FontWeight="Bold" Height="95" FontSize="25" LocalizableUnitText="@Units.s" LocalizableLabelText="@MachineOverview.Text28" RawLimitMax="59" RawLimitMin="0" FormatString="{}0" VariableName="CPU3.PLC.Blocks.03 Basket coating.01 CD.00 Main.Program.DB Program control.Actual.Spinning.1.Time"/>
