<UserControl x:Class="HMI.Resources.UserControls.MO.M1_MV_ST"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vw="http://inosoft.com/visiwin7" Opacity="0" Loaded="UserControl_Loaded" Visibility="Hidden">
    <Grid x:Name="A"   >
        <vw:PictureBox SymbolResourceKey="MV_ST" Width="140" Height="233" Margin="3" HorizontalAlignment="Left" VerticalAlignment="Top" >
            <vw:PictureBox.Effect>
                <DropShadowEffect Color="Black" BlurRadius="3"  Direction="270"  Opacity="0.5"/>
            </vw:PictureBox.Effect>
        </vw:PictureBox>
        <vw:TextBlock x:Name="nr1" Text="1" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="26,0,112,144" FontSize="16" FontWeight="Bold" Foreground="White" BlinkMode="Visibility" />

        <vw:Button Click="Button_Click_1" Margin="0"  Style="{DynamicResource B_Station}" />


    </Grid>

</UserControl>
